{
    "description": "Basic client-server edition. without filesystem and tiddlyweb.",
    "plugins": [
        "tiddlywiki/xmldom",
        "googleanalytics_2023",
        "statcounter",
        "tiddlywiki/innerwiki",
        "tiddlywiki/highlight",
        "_tiddlywiki/tiddlyweb",
        "_tiddlywiki/filesystem"
    ],
    "themes": [
        "tiddlywiki/vanilla",
        "tiddlywiki/snowwhite"
    ],
	"languages": [
		"ar-PS",
		"ca-ES",
		"cs-CZ",
		"da-DK",
		"de-AT",
		"de-CH",
		"de-DE",
		"el-GR",
		"en-US",
		"es-ES",
		"fa-IR",
		"fr-FR",
		"he-IL",
		"hi-IN",
		"ia-IA",
		"it-IT",
		"ja-JP",
		"ko-KR",
		"nl-NL",
		"pa-IN",
		"pl-PL",
		"pt-PT",
		"pt-BR",
		"ru-RU",
		"sk-SK",
		"sl-SI",
		"sv-SE",
		"zh-CN",
		"zh-TW",
		"zh-Hans",
		"zh-Hant"
	],
    "build": {
        "index": [
            "--rendertiddler",
            "$:/core/save/all",
            "index.html",
            "text/plain"
        ],
        "static": [
            "--rendertiddler",
            "$:/core/templates/static.template.html",
            "static.html",
            "text/plain",
            "--rendertiddler",
            "$:/core/templates/alltiddlers.template.html",
            "alltiddlers.html",
            "text/plain",
            "--rendertiddlers",
            "[!is[system]]",
            "$:/core/templates/static.tiddler.html",
            "static",
            "text/plain",
            "--rendertiddler",
            "$:/core/templates/static.template.css",
            "static/static.css",
            "text/plain"
        ]
    }
}
